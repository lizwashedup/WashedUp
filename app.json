{
  "expo": {
    "name": "washedup-app",
    "slug": "washedup-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "washedupapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.washedup.app",
      "config": {
        "googleMapsApiKey": "AIzaSyApjwAgT5x1pw5NgqSvrACmZaKapYuXgCw"
      }
    },
    "android": {
      "package": "com.washedup.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyApjwAgT5x1pw5NgqSvrACmZaKapYuXgCw"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow WashedUp to use your location to show plans near you."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
